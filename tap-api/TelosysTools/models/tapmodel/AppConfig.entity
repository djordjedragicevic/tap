@DbTable(app_config)
@DbCatalog(tap)
AppConfig {
  // attributes
  id : byte { @Id @AutoIncremented @DbName(id) @DbType(TINYINT UNSIGNED) @NotNull @NotEmpty };
  name : string { @DbName(name) @DbType("VARCHAR(32)") @NotNull @NotEmpty @Size(32) @MaxLen(32) };
  value : string { @DbName(value) @DbType("TEXT(65535)") @NotNull @NotEmpty @Size(65535) @MaxLen(65535) @LongText };
  level : byte { @DbName(level) @DbType(TINYINT) @DbDefaultValue(1) @NotNull @NotEmpty };
  // links
}
